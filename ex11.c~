#include <stdio.h>

int main(int argc, char *argv[]) {
	//got through each string in argv.
	
	int i = argc;
	while(i > 0) {
		printf("arg %d: %s.\n", i, argv[i]);
		i--;
	}
	
	//Lets make our own array of strings again.
	char *states[] = {
			"Washington", "Alabama",
			"Florida", "Nevada"};
	
	int numStates = 4;
	i = 0; //Attention here.
	while(i < numStates) {
		printf("state %d: %s.\n", i, states[i]);
		i++;
	}
	
	return 0;
	
}
	
